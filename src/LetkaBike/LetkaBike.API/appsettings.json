{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Error"
      }
    }
  },
  "ConnectionStrings": {
    "LetkaDatabase": "Server=(localdb)\\MSSQLLocalDB;Database=LetkaBike;Trusted_Connection=True;ConnectRetryCount=0"
  },
  "AppOptions": {
    "UseDB": "SqlServer"
  },
  "IdentityServer": {
    "Clients": {
      "LetkaBike": {
        "Profile": "IdentityServerSPA"
      }
    },
    "Key": {
      "Type": "Store",
      "StoreName": "My",
      "StoreLocation": "LocalMachine",
      "Name": "CN=letka.bike"
    }
  }
}
